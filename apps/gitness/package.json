{
  "name": "gitness",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint ./src",
    "pretty": "prettier --check ./src",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "pre-commit": "lint-staged",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "test:watch": "vitest",
    "test:changed": "vitest --run --changed=origin/main",
    "extract": "i18next --config './i18n.config.ts'",
    "typecheck": "tsc -b",
    "build:webpack": "rm -rf dist && webpack --config webpack.config.cjs",
    "start:webpack": "webpack serve --config webpack.config.cjs"
  },
  "dependencies": {
    "@harnessio/code-service-client": "3.5.0",
    "@harnessio/forms": "workspace:*",
    "@harnessio/pipeline-graph": "workspace:*",
    "@harnessio/ui": "workspace:*",
    "@harnessio/yaml-editor": "workspace:*",
    "@hookform/resolvers": "^3.6.0",
    "@tanstack/react-query": "4.20.4",
    "clipboard-copy": "^3.1.0",
    "clsx": "^2.1.1",
    "diff2html": "3.4.22",
    "event-source-polyfill": "^1.0.22",
    "highlight.js": "^11.11.1",
    "i18next": "^24.0.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "jotai": "^2.6.3",
    "lang-map": "^0.4.0",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.40.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "monaco-yaml": "^4.0.4",
    "pluralize": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.28.0",
    "react-i18next": "^15.1.1",
    "react-router-dom": "^6.26.0",
    "yaml": "^2.5.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@git-diff-view/react": "^0.0.16",
    "@swc/core": "^1.9.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^12.1.5",
    "@types/event-source-polyfill": "^1.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-istanbul": "^2.1.8",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "html-webpack-plugin": "^5.6.0",
    "immer": "^10.1.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.0.3",
    "raw-loader": "^4.0.2",
    "react-router-dom": "^6.26.0",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^6.0.3",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitest": "^2.1.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint ./src --fix",
      "prettier ./src --write"
    ]
  }
}
