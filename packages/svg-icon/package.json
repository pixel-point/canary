{
  "name": "@harnessio/svg-icon",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "clean": "clean-dist",
    "build": "pnpm run lint && tsc && esbuild src/*{.ts,tsx} --allow-overwrite --minify --outdir=dist --format=esm",
    "lint": "eslint .",
    "preinstall": "npx only-allow pnpm",
    "prepublishOnly": "pnpm build"
  },
  "module": "./dist/index.js",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/harness/canary.git"
  },
  "author": "Harness Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/harness/canary/issues"
  },
  "homepage": "https://github.com/harness/canary",
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@harnessio/svg-icon-cli": "workspace:*",
    "esbuild": "^0.23.0",
    "eslint": "^9.6.0",
    "globals": "^15.8.0",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.15.0"
  }
}
